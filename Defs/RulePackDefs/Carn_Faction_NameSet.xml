<?xml version="1.0" encoding="utf-8" ?>

<nameMakerDefs>


  <rulePackDef>
    <defName>Carn_Faction_NameSet</defName>
	
	<rulePack>
	  
	  <rulesStrings>
		<li>name->[leadername] and the [group]</li>
		<li>name->[leadername]'s [name2]</li>
		<li>name->[colour] [terrain] [carnival]</li>
		
		<li>name2->[cardinal] [carnival]</li>
		<li>name2->[colour] [carnival]</li>
		<li>name2->[strange] [carnival]</li>
		<li>name2->[carnival]</li>
		
		<li>carnival->Carnival</li>
		
		<li>leadername->[exciting] [personname]</li>
		<li>leadername->[personname]</li>
		
		<li>personname->[namefemale]</li>
		<li>personname->[namemale]</li>
		<li>personname-->[nameanimal]</li>
		
		<li>group->[group2]</li>
		<li>group->[group3]</li>
		<li>group->[group3]</li>
		<li>group->[strange] [group3]</li>
		<li>group->[strange] [group3]</li>
		<li>group->[strange] [group3]</li>
		<li>group->[colour] [group3]</li>
	  </rulesStrings>
	  
	  <!-- strings from language files -->
	  <rulesRaw>
	    
		<li Class="Rule_File">
		  <keyword>cardinal</keyword>
		  <pathList>
		    <li>Words/Adjectives/CompassDirections</li>
		  </pathList>
		</li>
		
		<li Class="Rule_File">
		  <keyword>colour</keyword>
		  <pathList>
		    <li>Words/Nouns/Colors</li>
		  </pathList>
		</li>
		
		<li Class="Rule_File">
		  <keyword>terrain</keyword>
		  <pathList>
		    <li>Words/Nouns/TerrainFeatures</li>
		  </pathList>
		</li>
		
		<li Class="Rule_File">
		  <keyword>nameanimal</keyword>
		  <pathList>
			<li>Names/Animal_Female</li>
			<li>Names/Animal_Male</li>
			<li>Names/Animal_Unisex</li>
		  </pathList>
		</li>
		
		<li Class="Rule_PersonName">
		  <gender>Male</gender>
		  <keyword>namemale</keyword>
		</li>
		
		<li Class="Rule_PersonName">
		  <gender>Female</gender>
		  <keyword>namefemale</keyword>
		</li>
		
		<li Class="Rule_File">
		  <keyword>group2</keyword>
		  <pathList>
		    <li>Words/Nouns/GroupNames</li>
		  </pathList>
		</li>
		
		<!-- words added by this mod -->
		
		<li Class="Rule_File">
		  <keyword>exciting</keyword>
		  <pathList>
		    <li>Carn_Words/Adjectives/Exciting</li>
		  </pathList>
		</li>
		
		<li Class="Rule_File">
		  <keyword>group3</keyword>
		  <pathList>
		    <li>Carn_Words/Nouns/Collectives</li>
		  </pathList>
		</li>
		
		<li Class="Rule_File">
		  <keyword>strange</keyword>
		  <pathList>
		    <li>Carn_Words/Adjectives/Strange</li>
		  </pathList>
		</li>
		
	  </rulesRaw>
	</rulePack>
	
  </rulePackDef>
</nameMakerDefs>