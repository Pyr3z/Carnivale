<?xml version="1.0" encoding="utf-8" ?>
<SpawnCategoryInjectorDefs>
  
  <!-->
  
  Use this format to inject custom spawn categories into existing backstories:
  
  <Xnope.Defs.SpawnCategoryInjectorDef>
    <defName>unique_name</defName>
    
    <newCategory>newCategory</newCategory>
	
	<injectToBackstories>
	  <li>full_title_of_backstory_1</li>
	  <li>full_title_of_backstory_2</li>
	</injectToBackstories>
  </Xnope.Defs.SpawnCategoryInjectorDef>
  
  <-->
  
  <Xnope.Defs.SpawnCategoryInjectorDef>
    <defName>SCI_Carnivale</defName>
    
    <newCategory>Carnivale</newCategory>
	<injectToBackstories>
	  <li>Actor</li>
	  <li>Con artist</li>
	  <li>Courtesan</li>
	  <li>Escaped convict</li>
	  <li>Musician</li>
	  <li>Medieval doctor</li>
	  <li>Medieval minstrel</li>
	  <li>Bodyguard</li>
	  <li>Caravaneer</li>
	  <!-- childhood -->
	  <li>Apocalypse survivor</li>
	  <li>Caravan traveler</li>
	  <li>Mute</li>
	  <li>Refugee</li>
	  <li>Child slave</li>
	  <li>Circus performer</li>
	  <li>Medieval thief</li>
	  <li>Sickly child</li>
	</injectToBackstories>
  </Xnope.Defs.SpawnCategoryInjectorDef>
  
  
  
  <Xnope.Defs.SpawnCategoryInjectorDef>
    <defName>SCI_CarnivaleTrader</defName>
	
    <newCategory>CarnivaleTrader</newCategory>
	<injectToBackstories>
	  <!-- Want to force Vendor adulthood backstory>
	  <li>Con artist</li>
	  <li>Bartender</li>
	  <li>Caravaneer</li>
	  <-->
	  <!-- childhood -->
	  <li>Shopkeeper</li>
	  <li>Caravan traveler</li>
	  <li>Circus performer</li>
	</injectToBackstories>
  </Xnope.Defs.SpawnCategoryInjectorDef>
  
  
  
  <Xnope.Defs.SpawnCategoryInjectorDef>
    <defName>SCI_CarnivaleFighter</defName>
    
    <newCategory>CarnivaleFighter</newCategory>
	<injectToBackstories>
	  <li>Bodyguard</li>
	  <li>Caravaneer</li>
	  <li>Castaway</li>
	  <li>Hunter</li>
	   <!-- childhood -->
	  <li>Apocalypse survivor</li>
	  <li>Caravan traveler</li>
	  <li>Refugee</li>
	  <li>Scout</li> <!-- Oops. There are two backstories named this. -->
	</injectToBackstories>
  </Xnope.Defs.SpawnCategoryInjectorDef>
  
  <Xnope.Defs.SpawnCategoryInjectorDef>
    <defName>SCI_CarnivaleWorker</defName>
    
    <newCategory>CarnivaleWorker</newCategory>
	<ignoreAdulthoods>true</ignoreAdulthoods>
	<injectToCategories>
	  <li>Civil</li>
	</injectToCategories>
  </Xnope.Defs.SpawnCategoryInjectorDef>

</SpawnCategoryInjectorDefs>